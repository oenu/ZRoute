{
  "name": "@zroute/client",
  "version": "0.1.0",
  "description": "Tool for mapping transit routes for acessible routin",
  "license": "GPL-3.0-only",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/zroute-client.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "dist/zroute-client.js",
  "module": "dist/zroute-client.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "api:bundle": "npx @redocly/cli bundle ../api/openapi.yaml -o ../api/bundled/openapi.yaml",
    "api:generate": "pnpm api:bundle && openapi-generator-cli generate -i ../api/bundled/openapi.yaml -g typescript-fetch -o src/generated --additional-properties=supportsES6=true,typescriptThreePlus=true",
    "build": "tsc -b && vite build",
    "dev": "vite",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.6.0",
    "@eslint/compat": "^2.0.2",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@openapitools/openapi-generator-cli": "^2.16.0",
    "@stylistic/eslint-plugin": "^5.8.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/utils": "^8.56.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^62.5.5",
    "eslint-plugin-n": "^17.24.0",
    "eslint-plugin-no-type-assertion": "^1.3.0",
    "eslint-plugin-package-json": "^0.88.2",
    "eslint-plugin-paths": "^1.1.0",
    "eslint-plugin-perfectionist": "^5.5.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-tsdoc": "^0.5.0",
    "eslint-plugin-unicorn": "^63.0.0",
    "globals": "^17.3.0",
    "jiti": "^2.6.1",
    "jsonc-eslint-parser": "^2.4.2",
    "prettier": "^3.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.56.0",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^4.0.0"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
